<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<!-- 
Employee e1 = new Employee();
e1.setId(1);
e1.setName("Ravi");
The below mentioned lines are equivalent to this
 -->
 
 <bean id="fruit" class="com.cg.demo4.Fruit">
 <property name="fruitnames">
 <list>
 <value>Apple</value>
 <value>Mango</value>
 <value>Banana</value>
 </list>
 </property>
 </bean>
<bean id="e1" class="com.cg.entity.Employee">
<property name="id" value="1"></property>
<property name="name" value="Ravi"></property>
</bean>
  
<bean id="s" class="com.cg.demo3.Student">
<constructor-arg value="1"></constructor-arg>
<constructor-arg value="Rahul"></constructor-arg>
</bean>
</beans>  