package day2;

public class ifDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int age = 2;
		if(age >= 18)
		{
			System.out.println("You are eligible");
			System.out.println("Thank you!");
		}
		else
		{
			System.out.println("You are not eligible");
		}

	}

}
